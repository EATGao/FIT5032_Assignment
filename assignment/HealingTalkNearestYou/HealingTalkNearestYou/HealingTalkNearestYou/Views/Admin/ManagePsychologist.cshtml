@model IEnumerable<HealingTalkNearestYou.Models.Psychologist>

@{
    ViewBag.Title = "ManagePsychologist";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Manage Psychologists</h2>

<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            Email
        </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Operation
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PsyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PsyEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PsyFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PsyLastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditPsy", new { id = item.PsyId }) |

            @Html.ActionLink("Delete", "DeletePsy", new { id = item.PsyId }) |

            @Html.ActionLink("Detail", "DetailPsy", new { id = item.PsyId })
        </td>
    </tr>
}

</table>
